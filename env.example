# DELO Configuration
# Copy this file to .env and fill in your API keys

# Google Gemini API Key (Required for intent classification)
GEMINI_API_KEY=your-gemini-api-key-here

# Optional: OpenAI API Key (if you want to use OpenAI instead of Gemini)
# OPENAI_API_KEY=your_openai_api_key_here

# Optional: Performance settings
# DELO_PERFORMANCE_MODE=ultra-lightweight
# DELO_OCR_ENABLED=true
# DELO_SCREENSHOT_INTERVAL=10000

# Optional: Logging
# DELO_LOG_LEVEL=info
# DELO_LOG_FILE=delo.log

# DELO Environment Configuration

# Development Settings
NODE_ENV=development

# Electron Settings
ELECTRON_IS_DEV=true 